@page
@model Przestepny_fixed.Pages.SavedInSessionModel
@{
	// wyswietlanie listy z sesji:
	for(int i =0; i < @Model.Sesja_.Lista_przestepnych.Count(); i++)
	{
		<div>
			@Model.Sesja_.Lista_przestepnych[i].Imie, @Model.Sesja_.Lista_przestepnych[i].Rok - rok @Model.Sesja_.Lista_przestepnych[i].czy_przestepny()
		</div>
	}
}
